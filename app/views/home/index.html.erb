<h1>Welcome</h1>

<% if user_signed_in? %>
  <p>Hello, <%= current_user.name.presence || current_user.email %>.</p>
  <p><%= link_to "View profile", profile_path %></p>
<% else %>
  <p><%= link_to "Sign in", new_user_session_path %> or <%= link_to "Sign up", new_user_registration_path %>.</p>
<% end %>


